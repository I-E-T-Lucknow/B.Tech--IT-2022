          <div class="stepwizard-step">
            <a style="color: rgba(232, 243, 234)";
              href="#step-3"
              type="button"
              class="btn btn-default btn-circle"
              disabled="disabled"
              >|</a
            >
            <p>Step 3</p>
          </div>
        </div>
      </div>
      <form  action="/users/newjobs/jobs" method="POST" id="jobFormD">
        <div class="row setup-content" id="step-1">
          <div class="col-xs-12">
            <div class="col-md-12">
              <h3 class="aj1">Step 1</h3>
              <div class="form-group">
                <label class="jobslabel"
                  >नौकरी का शीर्षक चुनें
                  </label
                >
                <div >
                  <input type="radio" id="checkbox" name="jobType" value="Painter" checked  />
                  <label for="checkbox" style="color: rgba(232, 243, 234);"><span>Painter</span></label>
                </div>

                <div >
                  <input
                    type="radio"
                    id="checkbox2"
                    value="Watchman"
                    name="jobType"                   />
                  <label for="checkbox2" style="color: rgba(232, 243, 234);"><span>Watchman</span></label>
                </div>
                <div >
                  <input
                    type="radio"
                    id="checkbox3"
                    value="Gardener"
                    name="jobType" 
                  />
                  <label for="checkbox3" style="color: rgba(232, 243, 234);"><span>Gardener</span></label>
                </div>

                <div  >
                  <input
                    type="radio"
                    id="checkbox4"
                    value="Maid"
                    name="jobType" 
                  />
                  <label for="checkbox4" style="color: rgba(232, 243, 234);"><span>Maid</span></label>
                </div>
              </div>
            
          </div>
          <button
          style="margin-left: 15px;"
          class="btn btn-3 mar-btn-1 nextBtn btn-lg pull-right"
          type="button"
        >
          Next
        </button>
        </div>
        
      </div>
        </div>
        <div class="row setup-content" id="step-2">
          <div class="col-xs-12">
            <div class="col-md-12">
              <h3 class="aj2">Step 2</h3>
              
              <div class="form-group">
                <label class="control-label">नौकरी का वर्णन करें</label>
                <textarea style="height: 130px;width: 550px;font-size: 15px;color: #254e58;"
                  maxlength="200"
                  type="text"
                  required="required"
                  name='jd'
                  class="form-control"
                  placeholder="Description"
                ></textarea>
              </div>
              <div>
              <button 
                class="btn btn-3 mar-btn-2 btn-4 nextBtn btn-lg pull-right"
                type="button"
              >
                Next
              </button>
            </div>
            </div>
            
          </div>
        </div>
        <div class="row setup-content" id="step-3">
          <div class="col-xs-12">
            <div class="col-md-12 xx">
              <h3 class="aj3">Step 3</h3>
              <div class="form-group">
                <div>
                <label class="jobslabel3"
                  >अंतिम सबमिट?<br> याद रखें कि आप एक ही प्रकार के कई जॉब ओपनिंग नहीं बना सकते हैं!</label
                ></div>
              <div>
              <button
              class="btn btn-3  btn-5 hara btn-lg pull-right" style="margin-bottom: 170px;"  id='jobSubmitD'  type="submit"
              >
              फिनिश
              </button>
            </div>
            </div>
            
          </div>
        </div>
      </form>
