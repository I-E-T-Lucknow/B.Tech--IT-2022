    <div class="container">
      <div class="stepwizard">
        <div class="stepwizard-row setup-panel">
          <div class="stepwizard-step">
            <a style="color: rgba(232, 243, 234);" href="#step-1" type="button" class="btn btn-primary btn-circle"
