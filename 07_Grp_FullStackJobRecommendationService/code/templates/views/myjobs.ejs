            <div class="panel-body">
              <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="list">
                  <table
                    class="table table-striped table-bordered table-list"
                    id="results"
                    style="width: 100%"
                  >
                    <thead>
                      <tr>
                        <th class="avatar" width="7%"></th>
                        <th width="20%">नौकरी का नाम</th>
                        <th width="70%">विवरण</th>
                        <th><em class="fa fa-cog"></em></th>
                      </tr>
                    </thead>
                    <tbody scrollbars="yes">
                      <% jobs.forEach((job)=>{%>

                      <tr class="ok">
                        <td class="avatar">
                          <%if(job.jobType=='Gardener') {%> <img src='/img/Gardener.jpg'/> <%}
                          else if(job.jobType=='Painter') {%> <img src='/img/Painter.jpg'/> <%}
                          else if(job.jobType=='Watchman') {%> <img src='/img/Watchman.jpg'/> <%}
                          else {%> <img src='/img/Maid.jpg'/> <%}%>
                        </td>
                        
                        <td class="aj" id="jobType" style='text-transform: capitalize;'> <%= job.jobType %> </td>
                        <td class="aj">
                          <%= job.jd%>
                        </td>
                        <td align="center">
                          <form method="POST" action="/jobs/delete" id="jobdeleteF">
                            <input type='hidden' name='_id' id="jobID" value= <%= job._id %> >
                            </input>
                            <button for='#jobdeleteF' class="btn btn-danger" title="delete" type="submit" id='jobdeleteD' > 
                             <i class='fa fa-trash'></i>
                            </button>
                          
                        </form>
                        </td>
                      </tr>
                    <% }) %>
                    </tbody>
                  </table>
                </div>
                <!-- END id="list" -->
