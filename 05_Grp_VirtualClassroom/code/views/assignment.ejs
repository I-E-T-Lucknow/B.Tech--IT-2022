        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <img src="/images/icon/logo.png" alt="Class Admin" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li style="background-color: #101010; padding-left: 20px; border-radius: 30px; margin-bottom: 10px;">
                            <a href="/dashboard" style="color: white;">
                                <i class="fas fa-chevron-left"></i>Back to dashboard</a>
                        </li>

                        <% subjectarray.forEach((one)=>{ %>
                            <li class="has-sub">
                                <a class="js-arrow" href="#">
                                    <%= one.name %>
                                </a>
                                <ul class="list-unstyled navbar__sub-list js-sub-list">
                                    <% one.assignments.forEach((oneassignment,index)=>{ %>
                                        <li>
                                            <a class="js-arrow" href="/assignments/<%= oneassignment._id %>">Assignment <%= index + 1 %></a>
                                        </li>
                                    <% }) %>
                                </ul>
                            </li>
                        <% }) %>

                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <div class="header-button">
                                <div class="noti-wrap">
                                    
                                    
                                    <div class="noti__item js-item-menu">
                                        <i class="zmdi zmdi-notifications"></i>
                                        <span class="quantity"><%= notificationarray.length %></span>
                                        <div class="notifi-dropdown js-dropdown">
                                            <div class="notifi__title">
                                                <p>You have <%= notificationarray.length %> Notifications</p>
                                            </div>

                                            <% notificationarray.forEach((one)=>{ %>
                                                <div class="notifi__item">
                                                    <div class="bg-c1 img-cir img-40">
                                                        <i class="zmdi zmdi-email-open"></i>
                                                    </div>
                                                    <div class="content">
                                                        <p><%= one.content %></p>
                                                        <span class="date">April 12, 2018 06:50</span>
                                                    </div>
                                                </div>
                                            <% }) %>

                                            <div class="notifi__footer">
                                                <a href="#">All notifications</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="account-wrap">
                                    <div class="account-item clearfix js-item-menu">
                                        <div class="image">
                                            <img src="/uploads/<%= user.image %>" alt="<%= user.name %>" />
                                        </div>
                                        <div class="content">
                                            <a class="js-acc-btn" href="#"><%= user.name %></a>
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="/uploads/<%= user.image %>" alt="<%= user.name %>" />
                                                    </a>
                                                </div>
                                                <div class="content">
                                                    <h5 class="name">
                                                        <a href="#"><%= user.name %></a>
                                                    </h5>
                                                    <span class="email"><%= user.email %></span>
                                                </div>
                                            </div>
                                            <div class="account-dropdown__footer">
                                                <a href="/user/logout">
                                                    <i class="zmdi zmdi-power"></i>Logout</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->

            <!-- MAIN CONTENT-->

            
            <!-- END MAIN CONTENT-->
            
