    <form action="/admin/addsubject" method="POST">
       <div class="form-group">
           <label for="exampleFormControlInput1">Name</label>
           <input type="text" class="form-control" id="exampleFormControlInput1" name="name" placeholder="John Doe">
       </div>
        <div class="form-group">
            <label for="exampleFormControlInput1">Faculty Name</label>
            <select name="faculty" class="form-control" id="exampleFormControlSelect1">
                <% facultyarray.forEach((one)=>{ %>
                    <option value="<%= one._id %>"><%= one.name %></option>
                <% }) %>
          </select>
       </div>
       <button type="submit" class="btn btn-primary">Submit</button>
   </form>


